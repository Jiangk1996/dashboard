{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Kubediag": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/frontend",
            "index": "src/app/frontend/index.html",
            "main": "src/app/frontend/main.ts",
            "polyfills": "src/app/frontend/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [{
              "glob": "**/*",
              "input": "src/app/frontend/assets",
              "output": "/assets"
            }],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "src/app/frontend/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [{
                "replace": "src/app/frontend/environments/environment.ts",
                "with": "src/app/frontend/environments/environment.prod.ts"
              }],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "Kubediag:build:production"
            },
            "development": {
              "browserTarget": "Kubediag:build:development",
              "proxyConfig": "proxy.conf.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Kubediag:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src//app/frontend/test.ts",
            "polyfills": "src/app/frontend/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [{
              "glob": "**/*",
              "input": "src/app/frontend/assets",
              "output": "/assets"
            }],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "src/app/frontend/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/app/frontend/**/*.ts", "src/app/frontend/**/*.html"]
          }
        }
      }
    }
  },
  "defaultProject": "Kubediag",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
