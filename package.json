{
  "name": "kubediag",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:frontend": "ng serve --port %npm_config_port% --host %npm_config_host%",
    "start:backend": "cd src/app/backend && npm run dev",
    "build:frontend": "ng build",
    "build:backend": "cd src/app/backend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "pre-commit": "lint-staged --no-stash"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "~13.2.4",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "~13.2.4",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "js-yaml": "^4.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@koa/router": "^10.1.1",
    "@kubernetes/client-node": "^0.16.3",
    "dotenv": "^16.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa2-cors": "^2.0.6",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.3.4",
    "typescript": "~4.5.2",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa2-cors": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint --fix"
    ]
  }
}
